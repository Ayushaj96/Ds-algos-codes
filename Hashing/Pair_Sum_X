#include <bits/stdc++.h>
using namespace std;

int main() {
	//code
	int T;
	cin>>T;
	
	while(T--){
	    int n,k;
	    cin>>n>>k;
	    
	    int arr[n];
	    unordered_map<int,int> m;
	    for(int i = 0; i < n; i++){
	        cin>>arr[i];
	        m[arr[i]]++;
	    }
	    int flag = 0;
	    for(int i = 0; i < n; i++){
	        if( k-arr[i] > 0 && m.find(k-arr[i]) != m.end()){
	            flag = 1;
	            if(k - arr[i] == arr[i] && m[arr[i]] < 2)
	                flag = 0;
	            break;
	        }
	    }
	    if(flag)
	        cout<<"Yes";
	    else
	        cout<<"No";
	    cout<<endl;     
	}
	return 0;
}
