#include <iostream>
using namespace std;

int candidate(int *arr, int n){
    int maj_index = 0 , count = 1;
    for(int i = 1; i < n; i++){
        if(arr[maj_index] == arr[i])
            count++;
        else
            count--;
        if(count == 0){
            maj_index = i;
            count = 1;
        }    
    }
    return arr[maj_index];
}
bool isMajority(int arr[], int n, int cand) 
{ 
    int count = 0; 
    for (int i = 0; i < n; i++){ 
        if (arr[i] == cand) 
            count++; 
    }
          
    if (count > n/2) 
        return true; 
        
    return false; 
} 

int majorityElement(int *arr, int n){
    int cand = candidate(arr,n);
    
    return isMajority(arr,n,cand) ? cand : -1;
}

int main() {
	//code
	int T;
	cin>>T;
	
	while(T--){
	    int n;
	    cin>>n;
	    
	    int arr[n];
	    for(int i = 0; i < n; i++)
	        cin>>arr[i];
	       
	    cout<<majorityElement(arr,n)<<endl;     
	}
	return 0;
}
