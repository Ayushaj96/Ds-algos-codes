#include <iostream>
#include <cstring>
using namespace std;

//recursive
int LCS(string str1, string str2, int n, int m){
    
    if(n == str1.length() || m == str2.length())
        return 0;
    
    if(str1[n] == str2[m])
        return 1+LCS(str1,str2,n+1,m+1);

    int val1 = LCS(str1,str2,n+1,m);
    int val2 = LCS(str1,str2,n,m+1);
    
    return max(val1,val2);
    
}

int main() {
	//code
	int T;
	cin>>T;
	
	while(T--){
	    int n,m;
	    cin>>n>>m;
	    
	    string str1, str2;
	    cin>>str1>>str2;
	    
	    cout<<LCS(str1,str2,0,0)<<endl;
	}
	return 0;
}
